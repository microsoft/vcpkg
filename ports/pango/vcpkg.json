{
  "name": "pango",
  "version": "1.48.4",
  "port-version": 1,
  "description": "Text and font handling library.",
  "homepage": "https://ftp.gnome.org/pub/GNOME/sources/pango/",
  "dependencies": [
    {
      "name": "cairo",
      "features": [
        "gobject"
      ]
    },
    "fontconfig",
    "freetype",
    "fribidi",
    "gettext",
    "glib",
    {
      "name": "glib",
      "host": true
    },
    "harfbuzz",
    {
      "name": "harfbuzz",
      "features": [
        "coretext"
      ],
      "platform": "osx"
    }
  ]
}
