{
  "name": "chromaprint",
  "version": "1.5.1",
  "port-version": 1,
  "description": "C library for generating audio fingerprints used by AcoustID",
  "homepage": "https://github.com/acoustid/chromaprint",
  "license": "LGPL-2.1-or-later",
  "supports": "!xbox",
  "dependencies": [
    {
      "name": "ffmpeg",
      "default-features": false,
      "features": [
        "avcodec",
        "avformat",
        "swresample"
      ]
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ]
}
