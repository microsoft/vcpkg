{
  "name": "quarter",
  "version": "1.2.3",
  "description": "Coin3D GUI binding for Qt",
  "homepage": "https://coin3d.github.io/quarter/",
  "license": "BSD-3-Clause",
  "supports": "!arm & !android & !uwp",
  "dependencies": [
    "coin",
    {
      "name": "qtbase",
      "default-features": false
    },
    {
      "name": "qttools",
      "default-features": false
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "qt5": {
      "description": "Build with Qt5",
      "dependencies": [
        {
          "name": "qt5-base",
          "default-features": false
        },
        {
          "name": "qt5-tools",
          "default-features": false
        }
      ]
    }
  }
}
