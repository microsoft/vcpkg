{
  "name": "neko-network",
  "version": "1.0.3",
  "description": "Neko Network is a modern, easy-to-use, and efficient C++20 network library built on top of libcurl.",
  "homepage": "https://github.com/moehoshio/NekoNetwork",
  "license": "MIT OR Apache-2.0",
  "dependencies": [
    "curl",
    "neko-function",
    "neko-schema",
    "neko-system",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "nlog"
  ],
  "features": {
    "nlog": {
      "description": "Use nlog as the default logger.",
      "dependencies": [
        "neko-log"
      ]
    }
  }
}
