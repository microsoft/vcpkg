{
  "name": "libxpm",
  "version": "3.5.17",
  "description": "XPM format pixmap library",
  "homepage": "https://gitlab.freedesktop.org/xorg/lib/libxpm",
  "license": "MIT",
  "supports": "windows | linux",
  "dependencies": [
    "libx11",
    "xorg-macros",
    "xproto"
  ],
  "features": {
    "gettext": {
      "description": "use xgettext to build source",
      "dependencies": [
        {
          "name": "gettext",
          "host": true,
          "features": [
            "tools"
          ]
        },
        "gettext-libintl"
      ]
    }
  }
}
