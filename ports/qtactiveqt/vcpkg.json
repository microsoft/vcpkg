{
  "name": "qtactiveqt",
  "version-semver": "6.2.0",
  "description": "ActiveQt",
  "homepage": "https://www.qt.io/",
  "supports": "windows",
  "dependencies": [
    {
      "name": "qtactiveqt",
      "host": true,
      "default-features": false
    },
    {
      "$comment": "also requires printsupport!",
      "name": "qtbase",
      "default-features": false,
      "features": [
        "gui",
        "widgets"
      ]
    }
  ],
  "features": {
    "qml": {
      "description": "Build QML imports",
      "dependencies": [
        {
          "name": "qtdeclarative",
          "default-features": false
        }
      ]
    }
  }
}
