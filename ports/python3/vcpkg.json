{
  "name": "python3",
  "version": "3.10.7",
  "port-version": 7,
  "description": "The Python programming language",
  "homepage": "https://github.com/python/cpython",
  "license": "Python-2.0",
  "supports": "!uwp & !mingw",
  "dependencies": [
    {
      "name": "bzip2",
      "platform": "!(windows & static)"
    },
    "expat",
    {
      "name": "gettext-libintl",
      "platform": "!windows"
    },
    {
      "name": "libffi",
      "platform": "!(windows & static)"
    },
    {
      "name": "libiconv",
      "platform": "!windows"
    },
    {
      "name": "liblzma",
      "platform": "!(windows & static)"
    },
    {
      "name": "libuuid",
      "platform": "!osx & !windows"
    },
    {
      "name": "openssl",
      "platform": "!(windows & static)"
    },
    {
      "name": "python3",
      "host": true
    },
    {
      "name": "sqlite3",
      "platform": "!(windows & static)"
    },
    {
      "name": "vcpkg-cmake-get-vars",
      "host": true,
      "platform": "android"
    },
    "zlib"
  ],
  "features": {
    "deprecated-win7-support": {
      "description": "Deprecated support for the Windows 7 platform -- may be removed at any time."
    }
  }
}
