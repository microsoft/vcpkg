{
  "name": "libavif",
  "version-semver": "1.1.1",
  "port-version": 1,
  "description": "Library for encoding and decoding AVIF files",
  "homepage": "https://github.com/AOMediaCodec/libavif",
  "license": "BSD-2-Clause AND Apache-2.0",
  "dependencies": [
    "libyuv",
    {
      "name": "vcpkg-cmake",
