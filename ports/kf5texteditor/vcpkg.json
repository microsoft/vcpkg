{
  "name": "kf5texteditor",
  "version": "5.116.0",
  "port-version": 1,
  "description": "Full text editor component",
  "homepage": "https://invent.kde.org/frameworks/ktexteditor",
  "documentation": "https://api.kde.org/ktexteditor-index.html",
  "dependencies": [
    "ecm",
    {
      "name": "gettext",
      "host": true,
      "features": [
        "tools"
      ]
    },
    "gettext-libintl",
    "kf5archive",
    "kf5config",
    "kf5guiaddons",
    "kf5i18n",
    "kf5kio",
    "kf5parts",
    "kf5sonnet",
    "kf5syntaxhighlighting",
    {
      "name": "qt5-base",
      "default-features": false
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ]
}
