{
  "name": "at-spi2-core",
  "version": "2.59.0",
  "description": "Base DBus XML interfaces for accessibility, the accessibility registry daemon, and atspi library.",
  "homepage": "https://www.gtk.org/",
  "license": null,
  "supports": "linux",
  "dependencies": [
    {
      "name": "dbus",
      "platform": "linux"
