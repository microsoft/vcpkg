{
  "name": "mongo-c-driver",
  "version": "1.27.2",
  "description": "Client library written in C for MongoDB.",
  "homepage": "https://github.com/mongodb/mongo-c-driver",
  "license": null,
  "supports": "!uwp",
  "dependencies": [
    "libbson",
    {
      "name": "mongo-c-driver",
      "features": [
        "openssl"
      ],
      "platform": "!windows & !osx & !ios"
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "zlib"
  ],
  "features": {
    "icu": {
      "description": "Enable ICU support, necessary to use non-ASCII usernames or passwords",
      "dependencies": [
        "icu"
      ]
    },
    "openssl": {
      "description": "Use OpenSSL (even on Windows or Apple systems)",
      "dependencies": [
        "openssl"
      ]
    },
    "snappy": {
      "description": "Enables snappy compressor support",
      "dependencies": [
        "snappy"
      ]
    },
    "zstd": {
      "description": "Enables zstd compressor support",
      "dependencies": [
        {
          "name": "pkgconf",
          "host": true
        },
        "zstd"
      ]
    }
  }
}
