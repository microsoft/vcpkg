{
  "name": "qwt",
  "version-semver": "6.2.0",
  "port-version": 3,
  "description": "qt widgets library for technical applications",
  "homepage": "https://sourceforge.net/projects/qwt",
  "dependencies": [
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "concurrent",
        "gui",
        "widgets"
      ]
    },
    "qtsvg",
    {
      "name": "qttools",
      "default-features": false,
      "features": [
        "designer"
      ]
    },
    {
      "name": "vcpkg-qmake",
      "host": true,
      "default-features": false
    }
  ]
}
