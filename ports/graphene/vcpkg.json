{
  "name": "graphene",
  "version": "1.10.8",
  "port-version": 5,
  "description": "A thin layer of types for graphic libraries.",
  "homepage": "https://www.gtk.org/",
  "license": "MIT",
  "dependencies": [
    {
      "name": "gettext",
      "host": true,
      "default-features": false,
      "features": [
        "tools"
      ]
    },
    "gettext-libintl",
    "glib",
    {
      "name": "glib",
      "host": true
    },
    {
      "name": "vcpkg-tool-meson",
      "host": true
    }
  ],
  "features": {
    "introspection": {
      "description": "build with introspection",
      "supports": "!static",
      "dependencies": [
        "gobject-introspection"
      ]
    }
  }
}
