{
  "name": "qtscxml",
  "version-semver": "6.2.0",
  "description": "SCXML (state machine notation) compiler and related tools",
  "homepage": "https://www.qt.io/",
  "dependencies": [
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "gui"
      ]
    },
    {
      "name": "qtscxml",
      "host": true,
      "default-features": false
    }
  ],
  "features": {
    "qml": {
      "description": "Build QML imports",
      "dependencies": [
        {
          "name": "qtdeclarative",
          "default-features": false
        }
      ]
    }
  }
}
