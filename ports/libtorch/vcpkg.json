{
  "name": "libtorch",
  "version-semver": "1.8.1",
  "description": "Tensors and Dynamic neural networks in Python with strong GPU acceleration",
  "homepage": "https://pytorch.org/",
  "supports": "x64 & (windows | osx | linux) & !static",
  "dependencies": [
    "cpuinfo",
    "eigen3",
    "fmt",
    "gflags",
    "glog",
    "lmdb",
    "onnx-optimizer",
    "protobuf",
    {
      "name": "protobuf",
      "host": true
    },
    "sleef",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "eigen3",
    "xnnpack"
  ],
  "features": {
    "cuda": {
      "description": "Build with NVIDIA CUDA GPU backend",
      "dependencies": [
        "cuda",
        "cudnn",
        {
          "name": "tensorpipe",
          "features": [
            "cuda"
          ],
          "platform": "linux | osx"
        }
      ]
    },
    "dist": {
      "description": "Use distributed with MPI, Gloo, libuv, TensorPipe",
      "dependencies": [
        {
          "name": "gloo",
          "platform": "linux"
        },
        {
          "name": "libuv",
          "platform": "windows | osx"
        },
        {
          "name": "tensorpipe",
          "platform": "linux | osx"
        }
      ]
    },
    "eigen3": {
      "description": "BLAS with Eigen 3"
    },
    "fbgemm": {
      "description": "Build with FBGEMM (quantized 8-bit server operators)",
      "dependencies": [
        "fbgemm"
      ]
    },
    "fftw3": {
      "description": "Build with FFTW (Fastest Fourier Transform in the West)",
      "dependencies": [
        "fftw3"
      ]
    },
    "leveldb": {
      "description": "Build with LevelDB",
      "dependencies": [
        "leveldb",
        "snappy"
      ]
    },
    "mkl": {
      "description": "BLAS with Intel MKL and MKLDNN (x86, x86_64, and AArch64)",
      "dependencies": [
        "intel-mkl"
      ]
    },
    "nnpack": {
      "description": "Build with NNPack",
      "dependencies": [
        "nnpack"
      ]
    },
    "opencl": {
      "description": "Build with OpenCL",
      "dependencies": [
        "opencl"
      ]
    },
    "opencv3": {
      "description": "Build with OpenCV 3.x",
      "dependencies": [
        "opencv3"
      ]
    },
    "python": {
      "description": "Build with Python3 + NumPy"
    },
    "qnnpack": {
      "description": "Build with QNNPack",
      "dependencies": [
        "qnnpack"
      ]
    },
    "tbb": {
      "description": "Build with Intel TBB",
      "dependencies": [
        "tbb"
      ]
    },
    "vulkan": {
      "description": "Build with Vulkan GPU backend",
      "dependencies": [
        "vulkan"
      ]
    },
    "xnnpack": {
      "description": "Build with XNNPack",
      "dependencies": [
        "xnnpack"
      ]
    },
    "zstd": {
      "description": "Build with ZSTD",
      "dependencies": [
        "zstd"
      ]
    }
  }
}
