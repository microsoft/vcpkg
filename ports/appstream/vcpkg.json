{
  "name": "appstream",
  "version": "1.0.6",
  "port-version": 1,
  "description": "Tools and libraries to work with AppStream metadata",
  "homepage": "https://www.freedesktop.org/software/appstream/docs",
  "license": "LGPL-2.1-or-later",
  "dependencies": [
    {
      "name": "curl",
      "default-features": false
    },
    {
      "name": "gettext",
      "host": true,
      "features": [
        "tools"
      ]
    },
    "glib",
    {
      "name": "glib",
      "host": true
    },
    {
      "name": "gperf",
      "host": true
    },
    {
      "name": "libxml2",
      "default-features": false
    },
    "libxmlb",
    "libyaml",
    {
      "name": "vcpkg-tool-meson",
      "host": true
    },
    "zstd"
  ]
}
