{
  "name": "lensfun",
  "version": "0.3.4",
  "port-version": 2,
  "description": "Provide a open source database of photographic lenses and their characteristics",
  "homepage": "https://lensfun.github.io/",
  "license": null,
  "supports": "!arm",
  "dependencies": [
    "glib",
    {
      "name": "vcpkg-cmake",
