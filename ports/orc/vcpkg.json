{
  "name": "orc",
  "version": "2.2.2",
  "description": "The smallest, fastest columnar storage for Hadoop workloads.",
  "homepage": "https://orc.apache.org/",
  "license": "Apache-2.0",
  "supports": "!uwp",
  "dependencies": [
    "lz4",
    "protobuf",
    {
      "name": "protobuf",
      "host": true
    },
    "snappy",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "zlib",
    "zstd"
  ],
  "features": {
    "tools": {
      "description": "Build tools",
      "dependencies": [
        {
          "name": "getopt-win32",
          "platform": "windows & !mingw"
        },
        {
          "name": "gettimeofday",
          "platform": "windows & !mingw"
        }
      ]
    }
  }
}
