 SDK/CMakeLists.txt | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/SDK/CMakeLists.txt b/SDK/CMakeLists.txt
index 47d9d9a7..20a67c33 100644
--- a/SDK/CMakeLists.txt
+++ b/SDK/CMakeLists.txt
@@ -2,9 +2,9 @@ project(SIMDIS_SDK)
 
 # Configure various version fields
 set(SIMDIS_SDK_MAJOR_VERSION 1)
-set(SIMDIS_SDK_MINOR_VERSION 22)
+set(SIMDIS_SDK_MINOR_VERSION 23)
 set(SIMDIS_SDK_REVISION_VERSION 0)
