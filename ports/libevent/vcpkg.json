{
  "name": "libevent",
  "version": "2.1.12",
  "port-version": 7,
  "description": "An event notification library",
  "homepage": "https://github.com/libevent/libevent",
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "thread"
  ],
  "features": {
    "openssl": {
      "description": "Support for openssl",
      "dependencies": [
        {
          "name": "libevent",
          "features": [
            "thread"
          ]
        },
        "openssl"
      ]
    },
    "thread": {
      "description": "Support for thread"
    }
  }
}
