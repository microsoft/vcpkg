{
  "name": "mongoose",
  "version": "7.18",
  "port-version": 1,
  "description": "Embedded web server / embedded networking library",
  "homepage": "https://cesanta.com/",
  "license": null,
  "supports": "!uwp",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "mbedtls": {
      "description": "Enable mbedTLS support",
      "dependencies": [
        "mbedtls"
      ]
    },
    "ssl": {
      "description": "Enable OpenSSL support",
      "dependencies": [
        "openssl"
      ]
    }
  }
}
