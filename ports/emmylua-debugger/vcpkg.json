{
  "name": "emmylua-debugger",
  "version": "1.8.7",
  "port-version": 13,
  "description": "High-performance cross-platform Lua debugger with full debugging features including breakpoints, step execution, variable watch, and stack trace",
  "homepage": "https://github.com/EmmyLua/EmmyLuaDebugger",
  "license": "MIT",
  "supports": "!uwp & !xbox & !android & !ios & !(arm & windows)",
  "dependencies": [
    "libuv",
    {
      "$if": "!features.luajit",
      "name": "lua"
    },
    {
      "$if": "features.luajit",
      "name": "luajit"
    },
    "nlohmann-json",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "features": {
    "luajit": {
      "description": "Use LuaJIT instead of standard Lua 5.4",
      "supports": "!uwp & !(arm64 & windows)"
    }
  }
}
