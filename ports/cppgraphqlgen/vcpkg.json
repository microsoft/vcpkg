{
  "name": "cppgraphqlgen",
  "version": "4.5.7",
  "port-version": 1,
  "description": "C++ GraphQL schema service generator",
  "homepage": "https://github.com/microsoft/cppgraphqlgen",
  "license": "MIT",
  "dependencies": [
    "boost-program-options",
    "pegtl",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": [
    "rapidjson"
  ],
  "features": {
    "rapidjson": {
      "description": "Build the graphqljson library with RapidJSON.",
      "dependencies": [
        "rapidjson"
      ]
    }
  }
}
