{
  "name": "libqglviewer",
  "version": "2.9.1",
  "port-version": 4,
  "description": [
    "libQGLViewer is an open source C++ library based on Qt that eases the creation of OpenGL 3D viewers.",
    "Attention: Last upstream activity in December 2022."
  ],
  "homepage": "https://github.com/GillesDebunne/libQGLViewer",
  "license": "GPL-2.0-or-later",
  "supports": "!android & !xbox",
  "dependencies": [
    "opengl",
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "gui",
        "opengl",
        "widgets"
      ]
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ]
}
