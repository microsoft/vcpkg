{
  "name": "vcpkg-ci-mimalloc",
  "version": "2.2.3",
  "description": "Validates mimalloc",
  "dependencies": [
    "mimalloc",
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "default-features": [
    "ci"
  ],
  "features": {
    "ci": {
      "description": "Test feature permutation",
      "dependencies": [
        {
          "name": "mimalloc",
          "features": [
            "override"
          ],
          "platform": "windows & staticcrt"
        },
        {
          "name": "mimalloc",
          "features": [
            "secure"
          ],
          "platform": "arm64 & static"
        }
      ]
    }
  }
}
