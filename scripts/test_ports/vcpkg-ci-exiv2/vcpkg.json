{
  "name": "vcpkg-ci-exiv2",
  "version-string": "ci",
  "description": "Port to validate exiv2 within CI",
  "homepage": "https://github.com/microsoft/vcpkg",
  "license": "MIT",
  "dependencies": [
    {
      "name": "exiv2",
      "default-features": false
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "default-features": [
    "all"
  ],
  "features": {
    "all": {
      "description": "Test all features",
      "dependencies": [
        {
          "name": "exiv2",
          "features": [
            "bmff",
            "nls",
            "png",
            "xmp"
          ]
        }
      ]
    }
  }
}
