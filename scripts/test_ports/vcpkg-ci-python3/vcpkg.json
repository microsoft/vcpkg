{
  "name": "vcpkg-ci-python3",
  "version-string": "ci",
  "description": "Port to test python3 in CI",
  "homepage": "https://github.com/microsoft/vcpkg",
  "license": "MIT",
  "dependencies": [
    {
      "name": "python3",
      "default-features": false
    },
    {
      "name": "python3",
      "host": true,
      "default-features": false
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "default-features": [
    "ci"
  ],
  "features": {
    "ci": {
      "description": "Test features in CI",
      "dependencies": [
        {
          "name": "python3",
          "features": [
            {
              "name": "extensions",
              "platform": "!(windows & staticcrt)"
            },
            {
              "name": "readline",
              "platform": "!windows"
            }
          ]
        }
      ]
    }
  }
}
