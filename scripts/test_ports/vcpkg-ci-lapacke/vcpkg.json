{
  "name": "vcpkg-ci-lapacke",
  "version-string": "ci",
  "description": "Port to force features of lapacke within CI",
  "homepage": "https://github.com/microsoft/vcpkg",
  "license": "MIT",
  "dependencies": [
    "lapacke",
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "default-features": [
    "all"
  ],
  "features": {
    "all": {
      "description": "Test all features",
      "dependencies": [
        {
          "name": "lapacke",
          "features": [
            "tmg"
          ]
        }
      ]
    }
  }
}
