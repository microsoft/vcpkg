{
  "name": "vcpkg-ci-lua",
  "version-string": "ci",
  "description": "Validate port lua",
  "dependencies": [
    {
      "name": "lua",
      "default-features": false,
      "features": [
        "tools"
      ]
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "default-features": [
    {
      "$comment": "The platform expression is tuned for vcpkg CI.",
      "name": "run-programs",
      "platform": "native | (!android & !arm64)"
    }
  ],
  "features": {
    "run-programs": {
      "description": "Run lua and user program"
    }
  }
}
