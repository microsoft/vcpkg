{
  "name": "vcpkg-ci-libigl",
  "version-string": "ci",
  "description": "Port to force features of libigl within CI",
  "homepage": "https://github.com/microsoft/vcpkg",
  "license": "MIT",
  "dependencies": [
    {
      "name": "libigl",
      "default-features": false
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    }
  ],
  "default-features": [
    "glfw",
    "imgui"
  ],
  "features": {
    "glfw": {
      "description": "libigl[glfw]",
      "dependencies": [
        {
          "name": "libigl",
          "default-features": false,
          "features": [
            "glfw"
          ]
        }
      ]
    },
    "imgui": {
      "description": "libigl[imgui]",
      "dependencies": [
        {
          "name": "libigl",
          "default-features": false,
          "features": [
            "imgui"
          ]
        }
      ]
    }
  }
}
