{
  "name": "vcpkg-ci-netgen",
  "version-string": "ci",
  "description": "Force non-default features of netgen within vcpkg CI",
  "homepage": "https://github.com/microsoft/vcpkg",
  "dependencies": [
    {
      "name": "netgen",
      "features": [
        "cgns",
        "jpeg",
        "mpeg",
        "occ"
      ]
    }
  ]
}
